    <wadl:resource path="/tools/_findReplacePropertyValue">
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Find an replace a property value across an entire entity class, e.g.]]>
<![CDATA[if a DocumentaryUnit has a property with name &quot;foo&quot; and value &quot;bar&quot;,]]>
<![CDATA[change the value to &quot;baz&quot; on all items.]]>
<![CDATA[<p/>]]>
<![CDATA[<strong>Warning: This is a sharp tool! Back up the whole database first!</strong>]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="type" style="query">
            <wadl:doc>
              <![CDATA[The type of entity]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="name" style="query">
            <wadl:doc>
              <![CDATA[The name of the property to find and replace]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="from" style="query">
            <wadl:doc>
              <![CDATA[The property value to change]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="to" style="query">
            <wadl:doc>
              <![CDATA[The new value]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[How many items have been changed]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/tools/_findReplacePropertyValueRE">
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Find an replace a regex-specified substring of a property value]]>
<![CDATA[across an entire entity class e.g.]]>
<![CDATA[if an Address has a property with name &quot;url&quot; and value &quot;www.foo.com/bar&quot;,]]>
<![CDATA[providing a regex value <tt>^www</tt> and replacement <tt>http://www</tt> will]]>
<![CDATA[give the property a value of &quot;http://www.foo.com/bar&quot;.]]>
<![CDATA[<p/>]]>
<![CDATA[<strong>Warning: This is a sharp tool! Back up the whole database first!</strong>]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="type" style="query">
            <wadl:doc>
              <![CDATA[The type of entity]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="name" style="query">
            <wadl:doc>
              <![CDATA[The name of the property to find and replace]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="pattern" style="query">
            <wadl:doc>
              <![CDATA[A regex specifying a substring of the property value]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="replace" style="query">
            <wadl:doc>
              <![CDATA[A replacement substring]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[How many items have been changed]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
