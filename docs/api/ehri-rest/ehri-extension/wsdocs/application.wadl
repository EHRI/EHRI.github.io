    <wadl:resource path="/permission/{userId}/scope/{id}">
      <wadl:param name="userId" style="template">
        <wadl:doc>
          <![CDATA[The user's permissions]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="id" style="template">
        <wadl:doc>
          <![CDATA[The scope ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Get the user's permissions for a given scope.]]>
        </wadl:doc>
        <wadl:request/>
        <wadl:response>
          <wadl:doc>
            <![CDATA[The matrix for the given scope]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Set a user's permissions on a content type with a given scope.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[the serialized permission list]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[The new permission matrix]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/tools/generateConcepts/{repositoryId}/{vocabularyId}">
      <wadl:param name="repositoryId" style="template">
        <wadl:doc>
          <![CDATA[the repository id]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="vocabularyId" style="template">
        <wadl:doc>
          <![CDATA[the target vocabulary]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Create concepts and links derived from the access points]]>
<![CDATA[on a repository's documentary units.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="apt" style="query">
            <wadl:doc>
              <![CDATA[the access point types to process]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="lang" style="query">
            <wadl:doc>
              <![CDATA[the language code of created concepts]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="single" style="query">
            <wadl:doc>
              <![CDATA[don't create concepts/links for access points that]]>
<![CDATA[are unique to a single item]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="tolerant" style="query">
            <wadl:doc>
              <![CDATA[proceed even if there are integrity errors due]]>
<![CDATA[to slug collisions in the created concepts]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[the number of links created]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
