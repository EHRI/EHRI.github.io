      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Create concepts and links derived from the access points]]>
<![CDATA[on a repository's documentary units.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="apt" style="query">
            <wadl:doc>
              <![CDATA[the access point types to process]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="lang" style="query">
            <wadl:doc>
              <![CDATA[the language code of created concepts]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="single" style="query">
            <wadl:doc>
              <![CDATA[don't create concepts/links for access points that]]>
<![CDATA[are unique to a single item]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="tolerant" style="query">
            <wadl:doc>
              <![CDATA[proceed even if there are integrity errors due]]>
<![CDATA[to slug collisions in the created concepts]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[the number of links created]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
