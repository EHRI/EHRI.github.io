          <wadl:param name="tolerant" style="query">
            <wadl:doc><![CDATA[whether or not to die on the first validation error]]></wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc><![CDATA[a JSON object showing how many records were created,
updated, or unchanged.]]></wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>

