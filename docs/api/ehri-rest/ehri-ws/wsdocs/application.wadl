    <wadl:resource path="/import/batch">
      <wadl:method name="PUT">
        <wadl:doc>
          <![CDATA[Update a batch of items via JSON containing (partial)]]>
<![CDATA[data bundles.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[a JSON document containing partial bundles containing]]>
<![CDATA[the needed data transformations]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
          <wadl:param name="scope" style="query">
            <wadl:doc>
              <![CDATA[the ID of there item's permission scope]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="tolerant" style="query">
            <wadl:doc>
              <![CDATA[whether to allow individual validation failures]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="version" style="query">
            <wadl:doc>
              <![CDATA[whether to create a version prior to delete]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="log" style="query">
            <wadl:doc>
              <![CDATA[an optional log message]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[an import log describing the changes committed]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="DELETE">
        <wadl:doc>
