      <wadl:param name="to" style="template"/>
      <wadl:method name="POST">
        <wadl:request>
          <wadl:param name="id" style="query"/>
        </wadl:request>
        <wadl:response>
          <wadl:representation mediaType="*/*"/>
          <wadl:representation mediaType="application/xml"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/entities/{id}/descriptions/{did}/access-points">
      <wadl:param name="id" style="template">
        <wadl:doc>
          <![CDATA[the parent item's ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="did" style="template">
        <wadl:doc>
          <![CDATA[the description's ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Create an access point on the given description, for the given]]>
<![CDATA[parent item.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[the access point data]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[the new access point]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/entities/{id}/descriptions/{did}/access-points/{apid}">
      <wadl:param name="id" style="template">
        <wadl:doc>
          <![CDATA[the parent item's ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="did" style="template">
        <wadl:doc>
          <![CDATA[the description's ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="apid" style="template">
        <wadl:doc>
          <![CDATA[the access point's ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="DELETE">
        <wadl:doc>
          <![CDATA[Delete the access point with the given ID, on the given description,]]>
<![CDATA[belonging to the given parent item.]]>
        </wadl:doc>
        <wadl:request/>
      </wadl:method>
    </wadl:resource>
  </wadl:resources>
</wadl:application>
