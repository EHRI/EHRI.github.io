    <wadl:resource path="/entities">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Fetch a list of items by their ID.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="id" style="query">
            <wadl:doc>
              <![CDATA[a list of string IDs]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="gid" style="query">
            <wadl:doc>
              <![CDATA[a list of graph number IDs]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A serialized list of items]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Fetch a list of items by their ID.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[a JSON-encoded list of IDs, which can consist of]]>
<![CDATA[either strings or (if a number) graph (long) ids.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A serialized list of items]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
