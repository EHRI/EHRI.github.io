    <wadl:resource path="/import/skos">
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Import a SKOS file, of varying formats, as specified by the &quot;language&quot;]]>
<![CDATA[column of the file extensions table <a href="https://jena.apache.org/documentation/io/">here</a>.]]>
<![CDATA[<p/>]]>
<![CDATA[Example:]]>
<![CDATA[<p/>]]>
<![CDATA[<pre>]]>
<![CDATA[{@code]]>
<![CDATA[curl -X POST \]]>
<![CDATA[-H "X-User: mike" \]]>
<![CDATA[--data-binary @skos-data.rdf \]]>
<![CDATA["http://localhost:7474/ehri/import/skos?scope=gb-my-vocabulary&log=testing&tolerant=true"]]>
<![CDATA[}]]>
<![CDATA[</pre>]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[a stream of SKOS data in a valid format.]]>
          </wadl:doc>
          <wadl:representation mediaType="*/*"/>
          <wadl:representation mediaType="application/xml"/>
          <wadl:param name="scope" style="query">
            <wadl:doc>
              <![CDATA[the id of the import scope (i.e. repository)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="tolerant" style="query">
            <wadl:doc>
              <![CDATA[whether or not to die on the first validation error]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="log" style="query">
            <wadl:doc>
              <![CDATA[log message for import. If this refers to an accessible local file]]>
<![CDATA[its contents will be used.]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="format" style="query">
            <wadl:doc>
              <![CDATA[the RDF format of the POSTed data]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[a JSON object showing how many records were created,]]>
<![CDATA[updated, or unchanged.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
